        <settingConfig type="numeric" min="100" max ="200" errorMessage="@Strings.maxGoodSgvError"/>
	</setting>
    <setting propertyKey="@Properties.minGoodSgv" title="@Strings.minGoodSgvS" prompt="@Strings.minGoodSgvHelp">
